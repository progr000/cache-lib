{
  "name": "maksym/cache-lib",
  "type": "library",
  "description": "cache lib for small framework",
  "keywords": [
    "cache",
    "lib"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Maksym Haivoronskyi",
      "email": "maksym.haivoronskyi@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "ext-json": "*",
    "maksym/config-lib": "@stable",
    "maksym/sess-cook-lib": "@stable"
  },
  "require-dev": {
    "phpunit/phpunit": "@stable"
  },
  "suggest": {
    "ext-memcached": "Allows to use memcached driver for caching"
  },
  "autoload": {
    "psr-4": {
      "Maksym\\Cache\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
